<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mdc.mim.user.mapper.UserLoginMapper">
    <select id="queryAll" resultType="com.mdc.mim.user.entity.UserEntity"> select * from user </select>

    <insert id="add" parameterType="com.mdc.mim.user.entity.UserEntity"> INSERT INTO user
        (passwdMd5, userName, nickName, registerDate) (MD5 (#{passwd}), #{userName}, #{nickName},
        #{registerDate}) </insert>

    <select id="queryByName" resultType="com.mdc.mim.user.entity.UserEntity"> select * from user
        where userName = #{userName} </select>

</mapper>